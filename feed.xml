---
layout: null
---
<?xml version="1.0" encoding="UTF-8"?>
<rss version="2.0" xmlns:atom="http://www.w3.org/2005/Atom">
  <channel>
    <title>{{ site.title | xml_escape }}</title>
    <description>{{ site.description | xml_escape }}</description>
    <link>{{ site.url }}{{ site.baseurl }}/</link>
    <atom:link href="{{ "/feed.xml" | prepend: site.baseurl | prepend: site.url }}" rel="self" type="application/rss+xml"/>
    <pubDate>{{ site.time | date_to_rfc822 }}</pubDate>
    <lastBuildDate>{{ site.time | date_to_rfc822 }}</lastBuildDate>
    <generator>Jekyll v{{ jekyll.version }}</generator>
    {% for post in site.posts limit:10 %}
      <item>
        <title>{{ networking.title | xml_escape }}</title>
        <description>{{ networking.content | xml_escape }}</description>
        <pubDate>{{ networking.date | date_to_rfc822 }}</pubDate>
        <link>{{ networking.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ networking.url | prepend: site.baseurl | prepend: site.url }}</guid>
        {% for tag in networking.tags %}
        <category>{{ tag | xml_escape }}</category>
        {% endfor %}
        {% for cat in networking.categories %}
        <category>{{ cat | xml_escape }}</category>
        {% endfor %}
      </item>
      <item>
        <title>{{ language.title | xml_escape }}</title>
        <description>{{ language.content | xml_escape }}</description>
        <pubDate>{{ language.date | date_to_rfc822 }}</pubDate>
        <link>{{ language.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ language.url | prepend: site.baseurl | prepend: site.url }}</guid>
        {% for tag in language.tags %}
        <category>{{ tag | xml_escape }}</category>
        {% endfor %}
        {% for cat in language.categories %}
        <category>{{ cat | xml_escape }}</category>
        {% endfor %}
      </item>
      <item>
        <title>{{ embedded.title | xml_escape }}</title>
        <description>{{ embedded.content | xml_escape }}</description>
        <pubDate>{{ embedded.date | date_to_rfc822 }}</pubDate>
        <link>{{ embedded.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ embedded.url | prepend: site.baseurl | prepend: site.url }}</guid>
        {% for tag in embedded.tags %}
        <category>{{ tag | xml_escape }}</category>
        {% endfor %}
        {% for cat in embedded.categories %}
        <category>{{ cat | xml_escape }}</category>
        {% endfor %}
      </item>
      <item>
        <title>{{ programme.title | xml_escape }}</title>
        <description>{{ programme.content | xml_escape }}</description>
        <pubDate>{{ programme.date | date_to_rfc822 }}</pubDate>
        <link>{{ programme.url | prepend: site.baseurl | prepend: site.url }}</link>
        <guid isPermaLink="true">{{ programme.url | prepend: site.baseurl | prepend: site.url }}</guid>
        {% for tag in programme.tags %}
        <category>{{ tag | xml_escape }}</category>
        {% endfor %}
        {% for cat in programme.categories %}
        <category>{{ cat | xml_escape }}</category>
        {% endfor %}
      </item>
    {% endfor %}
  </channel>
</rss>
